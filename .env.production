VITE_FRONTEND_FORGE_API_KEY=

# ============================================
# APLICAÇÃO
# ============================================
VITE_APP_TITLE=Fifty
VITE_APP_LOGO=/logo.svg
NODE_ENV=production
PORT=3000

# ============================================
# CORS (Para comunicação Frontend-Backend)
# ============================================
CORS_ORIGIN=https://seu-dominio.vercel.app
